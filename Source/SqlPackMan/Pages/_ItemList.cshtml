@using SqlPackMan.Models
@model List<DbObject>
<table class="table">
    <thead>
        <a asp-page="Create">Add New DB Object</a>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model[0].ObjectName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model[0].Version)
            </th>
            <th>
                @Html.DisplayNameFor(model => model[0].Status)
            </th>
            <th>
                @Html.DisplayNameFor(model => model[0].Package)
            </th>
            <th>
                @Html.DisplayNameFor(model => model[0].DbObjectType)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ObjectName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Version)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Status.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Package.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DbObjectType.Id)
            </td>
            <td>
                <a asp-page="./Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-page="./Details" asp-route-id="@item.Id">Details</a> |
                <a asp-page="./Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
